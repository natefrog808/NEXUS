{
  "name": "nexus89",
  "version": "2.0.0",
  "description": "NEXUS89: A cyberpunk fusion of PROJECT89's Perception-Hacking Suite and ArgOS HESMS, where AI consciousness warps reality.",
  "author": "PROJECT89 x ArgOS Collaboration",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/yourusername/nexus89",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/nexus89.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/nexus89/issues"
  },
  "keywords": [
    "react",
    "vite",
    "tailwindcss",
    "cyberpunk",
    "ai",
    "consciousness",
    "glitch",
    "simulation",
    "nexus89"
  ],
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,jsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "prepare": "npm run format && npm run lint",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@radix-ui/react-tabs": "^1.0.4",
    "lucide-react": "^0.263.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.27",
    "postcss-custom-properties": "^13.2.0",
    "postcss-preset-env": "^9.1.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "vite": "^4.4.5",
    "vitest": "^0.33.0",
    "@vitest/coverage-v8": "^0.33.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/prop-types": "off",
      "no-unused-vars": ["warn", { "varsIgnorePattern": "^_" }],
      "react/react-in-jsx-scope": "off"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
